{
    "name": "smajti1/fakturomat",
    "description": "Fakturomat system to create and manage invoices",
    "keywords": [
        "faktura",
        "invoice",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Hubert Dziubi≈Ñski",
            "email": "smajti1@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "ext-intl": "*",
        "ext-pdo": "*",
        "ext-zip": "*",
        "ext-mbstring": "*",
        "chrome-php/chrome": "^1.11",
        "cviebrock/eloquent-sluggable": "^10.0",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^v10.35",
        "laravel/tinker": "~2.1",
        "laravel/ui": "^4.0",
        "smajti1/laravel-wizard": "^1.5",
        "spatie/flysystem-dropbox": "^3.0",
        "spatie/laravel-backup": "^8.4"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "~3.2",
        "barryvdh/laravel-ide-helper": "~3.1",
        "fakerphp/faker": "~1.10",
        "filp/whoops": "~2.5",
        "larastan/larastan": "^2.0",
        "mockery/mockery": "~1.2",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan": "^1.2.0",
        "phpstan/phpstan-phpunit": "^1.2.0",
        "phpstan/phpstan-strict-rules": "^1.2.0",
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "App\\Wizard\\Steps\\": "app/Steps",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "artisan:ide-helper": "./artisan ide-helper:generate --no-interaction && ./artisan ide-helper:model --no-interaction --write",
        "phpunit": "./vendor/bin/phpunit",
        "phpstan": "vendor/bin/phpstan analyse --configuration=phpstan.neon --memory-limit=1G",
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "replace": {
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-intl-normalizer": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-uuid": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php83": "*"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
