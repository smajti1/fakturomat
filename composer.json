{
    "name": "smajti1/fakturomat",
    "description": "Fakturomat system to create and manage invoices",
    "keywords": [
        "faktura",
        "invoice",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Hubert Dziubi≈Ñski",
            "email": "smajti1@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-intl": "*",
        "ext-pdo": "*",
        "cviebrock/eloquent-sluggable": "^8.0",
        "fideloper/proxy": "~4.2",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^8.14",
        "laravel/tinker": "~2.1",
        "laravel/ui": "^3.1",
        "smajti1/laravel-wizard": "^1.5"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "~3.2",
        "barryvdh/laravel-ide-helper": "~2.6",
        "filp/whoops": "~2.5",
        "fakerphp/faker": "~1.10",
        "mockery/mockery": "~1.2",
        "phpstan/phpstan": "^0.12.30",
        "phpunit/phpunit": "~9.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "App\\Wizard\\Steps\\": "app/Steps",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyse --configuration=phpstan.neon --memory-limit=1G",
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
